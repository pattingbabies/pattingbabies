local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("Boxing League") -- Creates the window

local a = w:CreateFolder("Autofarm") -- Creates the folder(U will put here your buttons,etc)
local c = w:CreateFolder("Auto Upgrade") -- Creates the folder(U will put here your buttons,etc)
local b = w:CreateFolder("Credits") -- Creates the folder(U will put here your buttons,etc)

a:Label("Autofarm",{
    TextSize = 25; -- Self Explaining
    TextColor = Color3.fromRGB(255,255,255); -- Self Explaining
    BgColor = Color3.fromRGB(69,69,69); -- Self Explaining
    
}) 

b:Button(".gg/R5k6km6tAq",function()
    
end)

b:Button("Blora#5650",function()

end)

a:Toggle("Farm Strength",function(bool)
    local args = {
        [1] = {
            [1] = "Interact"
        }
    }
    
    workspace.Machinery.GRAB3.RemoteEvent:FireServer(unpack(args))
end)

a:Toggle("Farm Endurance",function(bool)
    local args = {
        [1] = {
            [1] = "Interact"
        }
    }
    
    workspace.Machinery.GRAB3.RemoteEvent:FireServer(unpack(args))
end)

c:Toggle("Upgrade Endurance",function(bool)
    while wait(8.0) do
    local args = {
        [1] = "Upgrade",
        [2] = "END"
    }
    
    game:GetService("ReplicatedStorage").Level:FireServer(unpack(args))
    end
end)

c:Toggle("Upgrade Strength",function(bool)
    while wait(8.0) do
    local args = {
        [1] = "Upgrade",
        [2] = "STR"
    }
    
    game:GetService("ReplicatedStorage").Level:FireServer(unpack(args))
end
end)

c:Toggle("Upgrade Speed",function(bool)
    while wait(8.0) do
    local args = {
        [1] = "Upgrade",
        [2] = "SPE"
    }
    
    game:GetService("ReplicatedStorage").Level:FireServer(unpack(args))
end
end)

c:Toggle("Upgrade Dexterity",function(bool)
    while wait(8.0) do
    local args = {
        [1] = "Upgrade",
        [2] = "DEX"
    }
    
    game:GetService("ReplicatedStorage").Level:FireServer(unpack(args))
end
end)




b:DestroyGui()
